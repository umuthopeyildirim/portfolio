    "@babel/eslint-parser": "^7.28.4",
    "@docusaurus/types": "^3.8.1",
    "eslint": "^9.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.32.0",
